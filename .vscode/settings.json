{
	"files.exclude": {
		"**/*.out": true,
		"**/*.exe": true,
		"**/*.log": true,
		"**/*.cbproj": true,
	},
	"code-runner.executorMap": {
		"c": "cd $dir && clang -lm -std=c11 $fileName -o $fileNameWithoutExt.out && $dir$fileNameWithoutExt.out",
		"cpp": "cd $dir && clang++ -lm -std=c++17 $fileName -o $fileNameWithoutExt.out && $dir$fileNameWithoutExt.out",
	},
}
